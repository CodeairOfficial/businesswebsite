{
  "name": "devcon-website",
  "description": "The annual conference for all Ethereum developers, researchers, thinkers, and makers.",
  "author": "efdevcon",
  "version": "0.1.0",
  "license": "MIT",
  "private": true,
  "scripts": {
    "prebuild": "sed -i \"s|%BRANCH_PLACEHOLDER%|$BRANCH|g\" ./static/admin/config.yml && sed -i \"s|local_backend: true||g\" ./static/admin/config.yml",
    "build": "gatsby build",
    "dev": "netlify dev",
    "start": "gatsby develop -H 0.0.0.0 -p 8000",
    "local-cms": "npx netlify-cms-proxy-server",
    "debug": "node node_modules/.bin/gatsby develop --inspect",
    "format": "prettier --write \"src/**/*.{ts,tsx,json}\"",
    "lint": "eslint src/**/*.{ts,tsx}",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "jest",
    "test-watch": "jest --watch",
    "browserstack": "node browserstack/test",
    "browserstack-ts": "ts-node --project ./tsconfig.json browserstack/test.ts"
  },
  "dependencies": {
    "@loadable/component": "^5.14.1",
    "@tippyjs/react": "^4.2.5",
    "canvas-confetti": "^1.4.0",
    "gatsby": "^3.2.1",
    "gatsby-image": "^3.2.0",
    "gatsby-plugin-intl": "^0.3.3",
    "gatsby-plugin-mailchimp": "^5.2.2",
    "gatsby-plugin-manifest": "^3.2.0",
    "gatsby-plugin-matomo": "^0.9.0",
    "gatsby-plugin-netlify-cms": "^5.2.0",
    "gatsby-plugin-react-helmet": "^4.2.0",
    "gatsby-plugin-react-svg": "^3.0.0",
    "gatsby-plugin-remove-serviceworker": "^1.0.0",
    "gatsby-plugin-robots-txt": "^1.5.5",
    "gatsby-plugin-sass": "^4.2.0",
    "gatsby-plugin-sharp": "^3.2.0",
    "gatsby-plugin-sitemap": "^3.2.0",
    "gatsby-plugin-typescript": "^3.2.0",
    "gatsby-remark-embed-video": "^3.1.1",
    "gatsby-remark-responsive-iframe": "^3.2.0",
    "gatsby-source-filesystem": "^3.2.0",
    "gatsby-transformer-json": "^3.2.0",
    "gatsby-transformer-remark": "^3.2.0",
    "gatsby-transformer-remark-frontmatter": "^0.1.0",
    "gatsby-transformer-sharp": "^3.2.0",
    "gatsby-transformer-yaml": "^3.2.0",
    "moment": "^2.29.1",
    "moment-timezone": "^0.5.33",
    "netlify-cms-app": "^2.14.38",
    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-helmet": "^6.1.0",
    "react-slick": "^0.27.14",
    "react-use-gesture": "^9.1.3",
    "slick-carousel": "^1.8.1"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.13.0",
    "@types/jest": "^26.0.22",
    "@types/loadable__component": "^5.13.3",
    "@types/node": "^14.14.37",
    "@types/react": "^16.9.56",
    "@types/react-dom": "^16.9.9",
    "@types/react-helmet": "^6.1.0",
    "@types/react-slick": "^0.23.4",
    "@types/react-test-renderer": "^16.9.3",
    "babel-jest": "^26.6.3",
    "babel-preset-gatsby": "^1.2.0",
    "browserstack-local": "^1.4.8",
    "gatsby-plugin-root-import": "^2.0.6",
    "gatsby-plugin-ts-config": "^1.1.5",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^26.6.3",
    "jest-dom": "^4.0.0",
    "netlify-cli": "^3.15.0",
    "playwright": "^1.10.0",
    "prettier": "^2.1.2",
    "react-test-renderer": "^17.0.2",
    "sass": "^1.32.8",
    "selenium-webdriver": "^4.0.0-beta.2",
    "ts-node": "^9.1.1",
    "typescript": "^4.2.3"
  },
  "jest": {
    "modulePathIgnorePatterns": [
      "node_modules"
    ]
  }
}
